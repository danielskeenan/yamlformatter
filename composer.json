{
  "name": "dragoonboots/yamlformatter",
  "description": "Format YAML files",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Dan Keenan",
      "email": "dk@dankeenan.org"
    }
  ],
  "require": {
    "php": "^8.0",
    "php-ds/php-ds": "^1.3",
    "symfony/console": "^5.2",
    "symfony/yaml": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "DragoonBoots\\YamlFormatter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DragoonBoots\\YamlFormatter\\tests": "tests"
    }
  }
}
