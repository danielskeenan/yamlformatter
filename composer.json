{
  "name": "dragoonboots/yamlformatter",
  "description": "Auto-format YAML files with refs",
  "type": "project",
  "license": "MIT",
  "homepage": "https://github.com/danielskeenan/yamlformatter",
  "readme": "README.md",
  "support": {
    "issues": "https://github.com/danielskeenan/yamlformatter/issues",
    "source": "https://github.com/danielskeenan/yamlformatter"
  },
  "authors": [
    {
      "name": "Dan Keenan",
      "email": "dk@dankeenan.org"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "ext-json": "*",
    "opis/json-schema": "^1.0",
    "php-ds/php-ds": "^1.3",
    "symfony/console": "^5.0",
    "symfony/finder": "^5.0",
    "symfony/yaml": "^5.0"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^9.5"
  },
  "suggest": {
    "ext-ds": "Allows a small speed/efficiency increase"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "DragoonBoots\\YamlFormatter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DragoonBoots\\YamlFormatter\\tests\\": "tests"
    }
  },
  "bin": [
    "bin/yamlformatter"
  ]
}
